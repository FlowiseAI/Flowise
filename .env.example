# Server Port
PORT=3000

# Database Type (sqlite, mysql, postgres)
DATABASE_TYPE=sqlite

# Database Connection URL
DATABASE_URL=file:./database.sqlite

# JWT Secret
JWT_SECRET=your-jwt-secret

# Flowise Username and Password
FLOWISE_USERNAME=admin
FLOWISE_PASSWORD=password

# API Key (optional)
FLOWISE_API_KEY=

# CORS Origin
CORS_ORIGIN=*

# Allowed Origins for iframe embedding
IFRAME_ORIGINS=*

# Supabase Configuration
SUPABASE_URL=your-supabase-url
SUPABASE_KEY=your-supabase-key

# Google ADK Configuration
GOOGLE_ADK_API_KEY=your-google-adk-api-key
GOOGLE_ADK_PROJECT_ID=your-google-adk-project-id

# Composio API Key
COMPOSIO_API_KEY=your-composio-api-key

# Optional: Redis for rate limiting and queue
REDIS_URL=
REDIS_PASSWORD=

# Optional: Enable queue mode for high throughput
MODE=
ENABLE_QUEUE=false
QUEUE_CONCURRENCY=5

# Optional: Enable metrics
ENABLE_METRICS=false
METRICS_PROVIDER=prometheus

# Optional: Configure logging
LOG_LEVEL=info
LOG_PATH=logs/

# Optional: Configure proxy
GLOBAL_PROXY_URL=
NUMBER_OF_PROXIES=0