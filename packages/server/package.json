        "socket.io": "^4.6.1",
        "sqlite3": "^5.1.6",
        "stripe": "^17.6.0",
        "turndown": "^7.2.0",
        "typeorm": "^0.3.17",
        "uuid": "^9.0.1",
        "winston": "^3.9.0"
    },
    "devDependencies": {
        "@jest/globals": "^29.7.0",
        "@jest/types": "^29.6.3",
        "@types/chai": "^4.3.0",
        "@types/content-disposition": "0.5.8",
        "@types/cors": "^2.8.18",
        "@types/crypto-js": "^4.1.1",
        "@types/express-session": "^1.18.1",
        "@types/express": "^4.17.13",
        "@types/jest": "^29.5.12",
