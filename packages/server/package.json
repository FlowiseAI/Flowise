{
    "name": "flowise",
    "version": "2.0.4",
    "description": "Flowiseai Server",
    "main": "dist/index",
    "types": "dist/index.d.ts",
    "bin": {
        "flowise": "./bin/run"
    },
    "files": [
        "bin",
        "marketplaces",
        "dist",
        "npm-shrinkwrap.json",
        "oclif.manifest.json",
        "oauth2.html"
    ],
    "oclif": {
        "bin": "flowise",
        "commands": "./dist/commands"
    },
    "scripts": {
        "build": "tsc",
        "start": "PORT=4000 run-script-os",
        "clean": "rimraf dist",
        "nuke": "rimraf dist node_modules .turbo",
        "start:windows": "cd bin && run start",
        "start:default": "cd bin && ./run start",
        "dev": "tsc-watch --noClear -p ./tsconfig.json --onSuccess \"pnpm start\"",
        "oclif-dev": "run-script-os",
        "oclif-dev:windows": "cd bin && dev start",
        "oclif-dev:default": "cd bin && ./dev start",
        "postpack": "shx rm -f oclif.manifest.json",
        "prepack": "pnpm build && oclif manifest && oclif readme",
        "typeorm": "typeorm-ts-node-commonjs",
        "typeorm:migration-generate": "pnpm typeorm migration:generate -d ./src/utils/typeormDataSource.ts",
        "typeorm:migration-run": "pnpm typeorm migration:run -d ./src/utils/typeormDataSource.ts",
        "typeorm:migration-show": "pnpm typeorm migration:show -d ./src/utils/typeormDataSource.ts",
        "watch": "tsc --watch",
        "version": "oclif readme && git add README.md",
        "cypress:open": "cypress open",
        "cypress:run": "cypress run",
        "e2e": "start-server-and-test dev http://localhost:3000 cypress:run",
        "cypress:ci": "START_SERVER_AND_TEST_INSECURE=1 start-server-and-test start https-get://localhost:3000 cypress:run",
        "test": "jest",
        "test:api": "jest test/api",
        "test:api:watch": "jest test/api --watch",
        "test:billing": "jest src/services/billing/__tests__/*.test.ts",
        "test:billing:watch": "jest src/services/billing/__tests__/*.test.ts --watch"
    },
    "keywords": [],
    "homepage": "https://flowiseai.com",
    "author": {
        "name": "Henry Heng",
        "email": "henryheng@flowiseai.com"
    },
    "engines": {
        "node": ">=18.15.0 <19.0.0 || ^20"
    },
    "license": "SEE LICENSE IN LICENSE.md",
    "dependencies": {
        "utils": "workspace:*",
        "@contentful/rich-text-from-markdown": "^15.16.15",
        "@oclif/core": "^3.26.4",
        "@types/lodash": "^4.14.202",
        "@types/pdf-parse": "^1.1.4",
        "@types/uuid": "^9.0.7",
        "async-mutex": "^0.4.0",
        "axios": "1.6.2",
        "content-disposition": "0.5.4",
        "contentful-management": "^11.7.3",
        "cors": "^2.8.5",
        "crypto-js": "^4.1.1",
        "langfuse": "3.32.0",
        "dotenv": "^16.0.0",
        "express": "^4.17.3",
        "express-basic-auth": "^1.2.1",
        "express-oauth2-jwt-bearer": "^1.6.0",
        "express-rate-limit": "^6.9.0",
        "express-session": "^1.18.1",
        "flowise-components": "workspace:^",
        "flowise-ui": "workspace:^",
        "google-auth-library": "^9.4.0",
        "googleapis": "^144.0.0",
        "http-errors": "^2.0.0",
        "http-status-codes": "^2.3.0",
        "langchainhub": "^0.0.11",
        "lodash": "^4.17.21",
        "mammoth": "^1.9.0",
        "moment": "^2.29.3",
        "moment-timezone": "^0.5.34",
        "multer": "^1.4.5-lts.1",
        "mysql2": "^3.9.2",
        "openai": "^4.51.0",
        "passport": "^0.7.0",
        "passport-google-oauth20": "^2.0.0",
        "pg": "^8.11.1",
        "posthog-node": "^3.5.0",
        "reflect-metadata": "^0.1.13",
        "sanitize-html": "^2.11.0",
        "stripe": "^17.6.0",
        "socket.io": "^4.6.1",
        "sqlite3": "^5.1.6",
        "typeorm": "^0.3.6",
        "uuid": "^9.0.1",
        "winston": "^3.9.0"
    },
    "devDependencies": {
        "@jest/globals": "^29.7.0",
        "@jest/types": "^29.6.3",
        "@types/content-disposition": "0.5.8",
        "@types/cors": "^2.8.12",
        "@types/crypto-js": "^4.1.1",
        "@types/express-session": "^1.18.1",
        "@types/jest": "^29.5.12",
        "@types/multer": "^1.4.7",
        "@types/passport": "^1.0.17",
        "@types/passport-google-oauth20": "^2.0.16",
        "@types/sanitize-html": "^2.9.5",
        "concurrently": "^7.1.0",
        "cypress": "^13.13.0",
        "jest": "^29.7.0",
        "nodemon": "^2.0.22",
        "oclif": "^3",
        "rimraf": "^5.0.5",
        "run-script-os": "^1.1.6",
        "shx": "^0.3.3",
        "start-server-and-test": "^2.0.3",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.7.0",
        "tsc-watch": "^6.0.4",
        "typescript": "^5.4.5"
    }
}
